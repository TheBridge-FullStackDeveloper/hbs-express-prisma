<link rel="stylesheet" href="../public/posts.css">

<div class="posts">
    <h1>POSTS</h1>
    <div>
        {{#each posts}}
        <h2>{{this.title}}</h2>
        <p>{{this.content}}</p>
        <p>{{formatDate this.createdAt}}</p>
        <p>username: {{author.username}}</p>
        {{/each}}
    </div>
</div>